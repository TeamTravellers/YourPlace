@using YourPlace.Models.ManagerModels;
@model HotelCreateModel;
@{

    ViewData["Title"] = "Welcome!";
    Layout = "_LayoutForManagers";
}

<style>
    .formichka{
        padding-top:10%;
    }
</style>
<form method="post" enctype="multipart/form-data" class="formichka" asp-controller="ManagerMenu" asp-action="UploadImage">
    <input type="text" value="@Model.ManagerID" />
    <input type="file" name="imgFile" />
    <input type="submit" value="Upload Image Files"/>
    <h4>@ViewData["Message"]</h4>
</form>
